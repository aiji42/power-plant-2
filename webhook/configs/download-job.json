{
  "taskGroups": [
    {
      "taskCount": "1",
      "parallelism": "1",
      "taskSpec": {
        "computeResource": {
          "cpuMilli": "1000",
          "memoryMib": "512"
        },
        "maxRetryCount": 0,
        "maxRunDuration": "3600s",
        "runnables": [
          {
            "container": {
              "imageUri": "gcr.io/power-plant-2/power-plant-2/download-job:latest",
              "entrypoint": ""
            }
          }
        ]
      }
    }
  ],
  "allocationPolicy": {
    "instances": [
      {
        "policy": {
          "machineType": "e2-small",
          "provisioningModel": "SPOT"
        }
      }
    ]
  },
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}